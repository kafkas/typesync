{
  "name": "typesync-cli",
  "version": "0.0.0",
  "description": "Autogenerate Firestore models",
  "scripts": {
    "build": "rm -rf lib && tsc -p tsconfig.build.json",
    "format": "prettier --write \"src/**/*.(js|ts|json)\"",
    "format-check": "prettier --check \"src/**/*.(js|ts|json)\"",
    "lint": "eslint src --ext .ts",
    "test": "npm run test:compile && npm run test:run",
    "test:compile": "tsc -p tsconfig.test.json",
    "test:run": "jest",
    "test-local": "npm run test:compile && npm run test:run -- --verbose"
  },
  "files": [
    "lib"
  ],
  "exports": {
    "./schema": "./lib"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/kafkas/typesync.git"
  },
  "author": "",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/kafkas/typesync/issues"
  },
  "homepage": "https://github.com/kafkas/typesync#readme",
  "devDependencies": {
    "@babel/core": "^7.17.0",
    "@babel/preset-env": "^7.16.11",
    "@babel/preset-typescript": "^7.16.7",
    "@trivago/prettier-plugin-sort-imports": "4.3.0",
    "@types/jest": "^29.5.10",
    "@types/lodash": "^4.14.178",
    "@types/node": "^20.11.24",
    "@typescript-eslint/eslint-plugin": "^7.1.0",
    "@typescript-eslint/parser": "^7.1.0",
    "babel-jest": "^27.5.1",
    "eslint": "^8.57.0",
    "eslint-config-prettier": "^9.1.0",
    "eslint-plugin-import": "^2.29.1",
    "eslint-plugin-node": "^11.1.0",
    "eslint-plugin-prettier": "^5.1.3",
    "jest": "^29.7.0",
    "lodash": "^4.17.21",
    "prettier": "^3.2.5",
    "ts-node": "^10.9.2",
    "typescript": "5.3.3",
    "yargs": "^17.7.2"
  },
  "dependencies": {
    "@proficient/ds": "^0.3.2",
    "yaml": "^2.4.0",
    "zod": "^3.22.4"
  }
}
