{
  "name": "typesync",
  "version": "0.0.0",
  "description": "Autogenerate Firestore models",
  "scripts": {
    "build": "tsc",
    "format": "npm run format:js",
    "format-check": "npx prettier --check \"./**/*.(js|ts|json)\"",
    "format:js": "prettier --write \"./**/*.(js|ts|json)\"",
    "lint": "npx eslint . --ext .js,.ts"
  },
  "files": [
    "lib"
  ],
  "exports": {
    "./schema": "./lib"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/kafkas/typesync.git"
  },
  "author": "",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/kafkas/typesync/issues"
  },
  "homepage": "https://github.com/kafkas/typesync#readme",
  "devDependencies": {
    "@proficient/ds": "^0.3.2",
    "@trivago/prettier-plugin-sort-imports": "^4.3.0",
    "@types/node": "^20.11.24",
    "@typescript-eslint/eslint-plugin": "^7.1.0",
    "@typescript-eslint/parser": "^7.1.0",
    "eslint": "^8.57.0",
    "eslint-config-prettier": "^9.1.0",
    "eslint-plugin-import": "^2.29.1",
    "eslint-plugin-node": "^11.1.0",
    "eslint-plugin-prettier": "^5.1.3",
    "prettier": "^3.2.5",
    "ts-node": "^10.9.2",
    "typescript": "5.3.3",
    "zod": "^3.22.4"
  }
}
