{
  "name": "typesync-cli",
  "version": "0.2.0",
  "description": "Autogenerate Firestore models",
  "bin": {
    "typesync": "lib/cli.js"
  },
  "keywords": [
    "typesync",
    "firebase",
    "firestore",
    "cloud firestore"
  ],
  "type": "module",
  "scripts": {
    "build": "rm -rf lib && tsc -p tsconfig.build.json",
    "build-local": "yarn run build && chmod +x lib/cli.js",
    "dev": "tsc --watch -p tsconfig.build.json",
    "format": "prettier --write \"src/**/*.(js|ts|json)\"",
    "format-check": "prettier --check \"src/**/*.(js|ts|json)\"",
    "generate-json-schema": "tsx scripts/generate-json-schema",
    "lint": "eslint src --ext .ts",
    "test": "yarn run test:compile && yarn run test:run",
    "test:compile": "tsc -p tsconfig.test.json",
    "test:run": "node --experimental-vm-modules node_modules/jest/bin/jest.js",
    "test-local": "yarn run test:compile && yarn run test:run --verbose"
  },
  "files": [
    "lib"
  ],
  "repository": {
    "type": "git",
    "url": "git+https://github.com/kafkas/typesync.git"
  },
  "author": "Anar Kafkas <anarkafkas@gmail.com>",
  "license": "AGPL-3.0-only",
  "bugs": {
    "url": "https://github.com/kafkas/typesync/issues"
  },
  "homepage": "https://github.com/kafkas/typesync#readme",
  "devDependencies": {
    "@babel/core": "^7.17.0",
    "@babel/preset-env": "^7.16.11",
    "@babel/preset-typescript": "^7.16.7",
    "@trivago/prettier-plugin-sort-imports": "4.3.0",
    "@types/ink": "^2.0.3",
    "@types/jest": "^29.5.10",
    "@types/lodash": "^4.14.178",
    "@types/node": "^20.11.24",
    "@types/pluralize": "^0.0.33",
    "@types/react": "^18.2.69",
    "@typescript-eslint/eslint-plugin": "^7.1.0",
    "@typescript-eslint/parser": "^7.1.0",
    "babel-jest": "^27.5.1",
    "eslint": "^8.57.0",
    "eslint-config-prettier": "^9.1.0",
    "eslint-plugin-node": "^11.1.0",
    "eslint-plugin-prettier": "^5.1.3",
    "eslint-plugin-require-extensions": "^0.1.3",
    "jest": "^29.7.0",
    "ts-jest": "^29.1.2",
    "ts-node": "^10.9.2",
    "tsx": "^4.7.2",
    "typescript": "5.3.3",
    "zod-to-json-schema": "^3.22.5"
  },
  "dependencies": {
    "@proficient/ds": "^0.3.2",
    "glob": "^10.3.12",
    "ink": "^4.4.1",
    "lodash": "^4.17.21",
    "pluralize": "^8.0.0",
    "prettier": "^3.2.5",
    "react": "^18.2.0",
    "yaml": "^2.4.0",
    "yargs": "^17.7.2",
    "zod": "^3.22.4"
  }
}
